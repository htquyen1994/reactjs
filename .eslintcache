 dùng thao tác event.\r\n    state = {\r\n        isLogin: false\r\n    }\r\n\r\n    // isLogin = false; //True là đã đăng nhập, false chưa đăng nhập\r\n    userName = 'cybersoft';\r\n\r\n\r\n    renderLogin = () => {\r\n        if (this.state.isLogin) {\r\n            return <a className=\"nav-link\">Hello {this.userName}</a>;\r\n        }\r\n\r\n        return <button onClick={()=>{\r\n            //Khi click vào button sẽ gọi hàm handleLogin\r\n            this.handleLogin();\r\n        }} className=\"nav-link bg-dark text-white\">\r\n            Đăng nhập\r\n        </button>\r\n\r\n    }\r\n\r\n    //Hàm sẽ thực thi khi người dùng click vào nút đăng nhập\r\n    handleLogin = () => {\r\n        // this.state.isLogin = true;//Không được set giá trị trực tiếp cho state\r\n\r\n        //this.setState() : Phương thức có sẵn của lớp đối tượng component dùng để gán lại giá trị mới cho state đồng thời render lại giao diện.\r\n\r\n\r\n        // let newState = {\r\n        //     isLogin: true\r\n        // }\r\n\r\n        //Tham số 1: là state với giá trị mới\r\n        //Tham số 2: là hàm thực thi sau khi setState xử lý xong\r\n        //setState là phương thức bất đồng bộ\r\n        this.setState({\r\n            isLogin: true\r\n        })\r\n\r\n\r\n    }\r\n\r\n\r\n    render() {\r\n        //Gõ b4-navbar-background -> html -> jsx\r\n        return (\r\n            <div className=\"container-fluid\">\r\n                <nav className=\"navbar navbar-expand-sm navbar-dark bg-primary\">\r\n                    <a className=\"navbar-brand\" href=\"#\">Navbar</a>\r\n                    <button className=\"navbar-toggler d-lg-none\" type=\"button\" data-toggle=\"collapse\" data-target=\"#collapsibleNavId\" aria-controls=\"collapsibleNavId\" aria-expanded=\"false\" aria-label=\"Toggle navigation\" />\r\n                    <div className=\"collapse navbar-collapse\" id=\"collapsibleNavId\">\r\n                        <ul className=\"navbar-nav mr-auto mt-2 mt-lg-0\">\r\n                            <li className=\"nav-item active\">\r\n                                <a className=\"nav-link\" href=\"#\">Home <span className=\"sr-only\">(current)</span></a>\r\n                            </li>\r\n                            <li className=\"nav-item\">\r\n                                {this.renderLogin()}\r\n\r\n\r\n                            </li>\r\n\r\n                        </ul>\r\n                        <form className=\"form-inline my-2 my-lg-0\">\r\n                            <input className=\"form-control mr-sm-2\" type=\"text\" placeholder=\"Search\" />\r\n                            <button className=\"btn btn-outline-success my-2 my-sm-0\" type=\"submit\">Search</button>\r\n                        </form>\r\n                    </div>\r\n                </nav>\r\n\r\n            </div>\r\n        )\r\n    }\r\n}\r\n","/Users/admin/Workspace/CybersStudy/reactjs/sources/reactfe53e-20201211/src/State/BaiTapChonXe.js",["187"],"import React, { Component } from 'react'\r\n\r\nexport default class BaiTapChonXe extends Component {\r\n\r\n    //state chứa giá trị thay đổi trên giao diện khi người dùng thao tác\r\n    state = {\r\n        imgSrc: './img/imgCar/imgBlackCar.jpg' //Giá trị ban đầu\r\n    }\r\n\r\n    changeColor = (color) => {\r\n        this.setState({\r\n            imgSrc: `./img/imgCar/img${color}Car.jpg`\r\n        })\r\n    }\r\n\r\n    render() {\r\n        return (\r\n            <div className=\"container\">\r\n                <h1 className=\"text-center display-4\">Bài tập chọn màu xe</h1>\r\n                <div className=\"row\">\r\n                    <div className=\"col-6\">\r\n                        <img style={{width:'100%'}} src={this.state.imgSrc} />\r\n                    </div>\r\n                    <div className=\"col-6\">\r\n                        <div className=\"row\">\r\n                            <div className=\"col-4\">\r\n                                <button onClick={()=>{\r\n                                    this.changeColor('Red');\r\n                                }} style={{color:'#fff',backgroundColor:'red'}}>Red color</button>\r\n                            </div>\r\n                            <div className=\"col-4\">\r\n                                <button onClick={()=>{\r\n                                    this.changeColor('Silver');\r\n\r\n                                }} style={{color:'#fff',backgroundColor:'silver'}}>Silver color</button>\r\n                            </div>\r\n                            <div className=\"col-4\">\r\n                                <button onClick={()=>{\r\n                                    this.changeColor('Black');\r\n                                }} style={{color:'#fff',backgroundColor:'black'}}>Black color</button>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        )\r\n    }\r\n}\r\n","/Users/admin/Workspace/CybersStudy/reactjs/sources/reactfe53e-20201211/src/RenderWithMap/RenderWithMap.js",["188"],"import React, { Component } from 'react'\r\n\r\nconst mangSinhVien = [\r\n    { ma: 1, ten: 'Nguyễn Văn A', avatar: 'https://picsum.photos/50' },\r\n    { ma: 2, ten: 'Nguyễn Văn B', avatar: 'https://picsum.photos/50' },\r\n    { ma: 3, ten: 'Nguyễn Văn C', avatar: 'https://picsum.photos/50' },\r\n]\r\nexport default class RenderWithMap extends Component {\r\n\r\n    renderTable = () => {\r\n        // const mangTrSinhVien = []; // [<tr>123</tr>,<tr>2313</tr>]\r\n        // for (let index = 0; index < mangSinhVien.length; index++) {\r\n        //     //Mỗi lần duyệt lấy ra 1 đối tượng sinh viên\r\n        //     let sinhVien = mangSinhVien[index];\r\n        //     //tạo ra thẻ jsx Tr sinh viên\r\n        //     let jsxTr = <tr key={index}>\r\n        //         <td>{sinhVien.ma}</td>\r\n        //         <td>{sinhVien.ten}</td>\r\n        //         <td><img src={sinhVien.avatar} /></td>\r\n        //     </tr>;\r\n        //     //push đối tượng vào mangTr\r\n        //     mangTrSinhVien.push(jsxTr);\r\n        // }\r\n        const mangTrSinhVien = mangSinhVien.map((sinhVien, index) => {\r\n            return <tr key={index}>\r\n                <td>{sinhVien.ma}</td>\r\n                <td>{sinhVien.ten}</td>\r\n                <td><img src={sinhVien.avatar} /></td>\r\n            </tr>\r\n        })\r\n\r\n        return mangTrSinhVien;\r\n    }\r\n    render() {\r\n        return (\r\n            <div className=\"container\">\r\n                <table className=\"table\">\r\n                    <thead>\r\n                        <tr>\r\n                            <th>Mã</th>\r\n                            <th>Tên</th>\r\n                            <th>Hình ảnh</th>\r\n                        </tr>\r\n                    </thead>\r\n                    <tbody>\r\n                        {this.renderTable()}\r\n                    </tbody>\r\n                </table>\r\n            </div>\r\n        )\r\n    }\r\n}\r\n","/Users/admin/Workspace/CybersStudy/reactjs/sources/reactfe53e-20201211/src/Databinding/Databinding.js",["189","190"],"import React, { Component } from 'react'\r\n\r\nexport default class Databinding extends Component {\r\n\r\n    //Thuộc tính sinh viên\r\n    sinhVien = {\r\n        ma: 1,\r\n        ten: 'Nguyễn văn a',\r\n        lop: 'FrontEnd53E',\r\n        avatar: 'https://picsum.photos/200'\r\n    }\r\n\r\n\r\n    //Phương thức \r\n    renderCardSinhVien = (sv) => {\r\n        return <div>\r\n            <ul>\r\n                <li>Mã sinh viên: {sv.ma}</li>\r\n                <li>Tên sinh viên: {sv.ten}</li>\r\n                <li>Lớp sinh viên: {sv.lop}</li>\r\n            </ul>\r\n        </div>\r\n    }\r\n\r\n\r\n    render() {\r\n        const title = \"cybersoft\";\r\n        const renderTitle = () => {\r\n            //Xử lý ....\r\n            //Nội dung trả về có thể là chuỗi hoặc số hoặc thẻ html (phải được bao phủ bởi 1 thẻ)\r\n            return \"Hello cybersoft\";\r\n        }\r\n        const renderImg = () => {\r\n\r\n            return <div>\r\n                <img src=\"https://picsum.photos/200\" />\r\n            </div>\r\n        }\r\n        return (\r\n            <div className=\"container\">\r\n                <p id=\"title\">{title}</p>\r\n                <h1>{renderTitle()}</h1>\r\n                {renderImg()}\r\n                <div className=\"w-25 card\">\r\n                    <div className=\"card-header\">{this.sinhVien.ten}</div>\r\n                    <div className=\"card-body\">\r\n                        <img src={this.sinhVien.avatar} style={{ width: '100%' }} />\r\n                    </div>\r\n                    <div className=\"card-footer\">\r\n                        {this.sinhVien.lop}\r\n                    </div>\r\n                </div>\r\n\r\n                {this.renderCardSinhVien(this.sinhVien)}\r\n            </div>\r\n        )\r\n    }\r\n}\r\n","/Users/admin/Workspace/CybersStudy/reactjs/sources/reactfe53e-20201211/src/Props/DemoProps/DemoProps.js",[],"/Users/admin/Workspace/CybersStudy/reactjs/sources/reactfe53e-20201211/src/Component/BaiTapLayout/Carousel.js",[],"/Users/admin/Workspace/CybersStudy/reactjs/sources/reactfe53e-20201211/src/Component/BaiTapLayout/BTHeader.js",["191","192","193","194","195","196","197"],"import React, { Component } from 'react'\r\n\r\nexport default class BTHeader extends Component {\r\n\r\n    render() {\r\n        return (\r\n            <nav className=\"navbar navbar-expand-md navbar-dark bg-dark\">\r\n                <div className=\"container-fluid\">\r\n                    <a href=\"#\" className=\"navbar-brand\">CyberSoft</a>\r\n                    <button className=\"navbar-toggler\" data-toggle=\"collapse\" data-target=\"#myMenu\">\r\n                        <span className=\"navbar-toggler-icon\" />\r\n                    </button>\r\n                    <div className=\"collapse navbar-collapse\" id=\"myMenu\">\r\n                        <ul className=\"navbar-nav ml-auto\">\r\n                            <li className=\"nav-item\">\r\n                                <a href=\"#\" className=\"nav-link active\">Home</a>\r\n                            </li>\r\n                            <li className=\"nav-item\">\r\n                                <a href=\"#\" className=\"nav-link\">News</a>\r\n                            </li>\r\n                            <li className=\"nav-item dropdown\">\r\n                                <a href=\"#\" className=\"nav-link\" data-toggle=\"dropdown\">Products</a>\r\n                                <div className=\"dropdown-menu\">\r\n                                    <a href=\"#\" className=\"dropdown-item\">Smarphone</a>\r\n                                    <a href=\"#\" className=\"dropdown-item\">Laptop</a>\r\n                                </div>\r\n                            </li>\r\n                            <li className=\"nav-item\">\r\n                                <a href=\"#\" className=\"nav-link\">Forum</a>\r\n                            </li>\r\n                        </ul>\r\n                    </div>\r\n                </div>\r\n            </nav>\r\n\r\n        )\r\n    }\r\n}\r\n","/Users/admin/Workspace/CybersStudy/reactjs/sources/reactfe53e-20201211/src/Component/BaiTapLayout/BTProduct.js",["198","199","200"],"import React, { Component } from 'react'\r\n\r\nexport default class BTProduct extends Component {\r\n    render() {\r\n        return (\r\n            <div className=\"container\">\r\n                <div className=\"card bg-light\" style={{ width: 300 }}>\r\n                    <img className=\"card-img-top\" src=\"./img/sp_iphoneX.png\" alt=\"Card image\" style={{ maxWidth: '100%', height: 250 }} />\r\n                    <div className=\"card-body text-center\">\r\n                        <h4 className=\"card-title text-center\">iPhone X</h4>\r\n                        <p className=\"card-text\">iPhone X features a new all-screen design. Face ID, which makes your face your password</p>\r\n                        <a href=\"#\" className=\"btn btn-primary\">Detail</a>\r\n                        <a href=\"#\" className=\"btn btn-danger\">Cart</a>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n\r\n        )\r\n    }\r\n}\r\n","/Users/admin/Workspace/CybersStudy/reactjs/sources/reactfe53e-20201211/src/Component/BaiTapLayout/Footer.js",["201","202","203"],"import React, { Component } from 'react'\r\n\r\nexport default class Footer extends Component {\r\n    render() {\r\n        return (\r\n            <section id=\"promotion\" className=\"container-fluid pt-5 pb-5 bg-dark\">\r\n                <h1 className=\"text-center text-white\">PROMOTION</h1>\r\n                <div className=\"container bg-light pt-5 pb-5\">\r\n                    <div className=\"row\">\r\n                        <div className=\"col-xs-12 col-sm-12 col-md-4\">\r\n                            <div className=\"container\">\r\n                                <img src=\"./img/promotion_1.png\" alt style={{ maxWidth: '100%' }} />\r\n                            </div>\r\n                        </div>\r\n                        <div className=\"col-xs-12 col-sm-12 col-md-4\">\r\n                            <div className=\"container\">\r\n                                <img src=\"./img/promotion_2.png\" alt style={{ maxWidth: '100%' }} />\r\n                            </div>\r\n                        </div>\r\n                        <div className=\"col-xs-12 col-sm-12 col-md-4\">\r\n                            <div className=\"container\">\r\n                                <img src=\"./img/promotion_3.jpg\" alt style={{ maxWidth: '100%' }} />\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </section>\r\n\r\n        )\r\n    }\r\n}\r\n","/Users/admin/Workspace/CybersStudy/reactjs/sources/reactfe53e-20201211/src/Component/BaiTapLayout/BTProductList.js",[],"/Users/admin/Workspace/CybersStudy/reactjs/sources/reactfe53e-20201211/src/Props/DemoProps/SanPham.js",["204","205"],"import React from 'react'\n\nexport default function SanPham(props) {\n    const {ma, tenSp, img, gia} = props.sp;\n    return (\n        <div className=\"card text-white bg-primary\">\n            <img className=\"card-img-top\" src={img} height=\"350\">\n\n            </img>\n\n            <div className=\"card-body\">\n                <h4 className=\"card-title\">{tenSp}</h4>\n                <p className=\"card-text\">{gia}</p>\n            </div>\n        </div>\n    )\n}\n","/Users/admin/Workspace/CybersStudy/reactjs/sources/reactfe53e-20201211/src/Props/BTDanhSachSP/BTProductList.js",["206"],"import React, { Component } from 'react'\nimport BTProduct from './BTProduct'\nimport Cart from './Cart'\n\nconst mangSanPham = [\n    {\n        \"maSP\": 1,\n        \"tenSP\": \"VinSmart Live\",\n        \"manHinh\": \"AMOLED, 6.2\\\", Full HD+\",\n        \"heDieuHanh\": \"Android 9.0 (Pie)\",\n        \"cameraTruoc\": \"20 MP\",\n        \"cameraSau\": \"Chính 48 MP & Phụ 8 MP, 5 MP\",\n        \"ram\": \"4 GB\",\n        \"rom\": \"64 GB\",\n        \"giaBan\": 5700000,\n        \"hinhAnh\": \"./img/vsphone.jpg\"\n    },\n\n    {\n        \"maSP\": 2,\n        \"tenSP\": \"Meizu 16Xs\",\n        \"manHinh\": \"AMOLED, FHD+ 2232 x 1080 pixels\",\n        \"heDieuHanh\": \"Android 9.0 (Pie); Flyme\",\n        \"cameraTruoc\": \"20 MP\",\n        \"cameraSau\": \"Chính 48 MP & Phụ 8 MP, 5 MP\",\n        \"ram\": \"4 GB\",\n        \"rom\": \"64 GB\",\n        \"giaBan\": 7600000,\n        \"hinhAnh\": \"./img/meizuphone.jpg\"\n    },\n\n    {\n        \"maSP\": 3,\n        \"tenSP\": \"Iphone XS Max\",\n        \"manHinh\": \"OLED, 6.5\\\", 1242 x 2688 Pixels\",\n        \"heDieuHanh\": \"iOS 12\",\n        \"cameraSau\": \"Chính 12 MP & Phụ 12 MP\",\n        \"cameraTruoc\": \"7 MP\",\n        \"ram\": \"4 GB\",\n        \"rom\": \"64 GB\",\n        \"giaBan\": 27000000,\n        \"hinhAnh\": \"./img/applephone.jpg\"\n    }\n]\nexport default class BTProductList extends Component {\n\n    state = {\n        detailProduct: {\n            \"maSP\": 1,\n            \"tenSP\": \"VinSmart Live\",\n            \"manHinh\": \"AMOLED, 6.2\\\", Full HD+\",\n            \"heDieuHanh\": \"Android 9.0 (Pie)\",\n            \"cameraTruoc\": \"20 MP\",\n            \"cameraSau\": \"Chính 48 MP & Phụ 8 MP, 5 MP\",\n            \"ram\": \"4 GB\",\n            \"rom\": \"64 GB\",\n            \"giaBan\": 5700000,\n            \"hinhAnh\": \"./img/vsphone.jpg\"\n        },\n\n        gioHang: [\n            {maSP: 3, tenSP: 'IPhoneX', hinhAnh: \"./img/applephone.jpg\", giaBan: 2000, soLuong: 3}\n        ] //dữ liệu giõ hàng\n    }\n\n    xemChiTiet = (product) => {\n        this.setState({detailProduct: product})\n    }\n\n    render() {\n        let { detailProduct } = this.state;\n        return (\n            <div className=\"container\">\n                <h1 className=\"text-center\">BEST SMARTPHONE</h1>\n                <div className=\"row\">\n                    <div className=\"col-12 text-right\">\n                        <span style={{cursor:'pointer'}} data-toggle=\"modal\" data-target=\"#modelId\">\n                            Giỏ hàng ({this.getTotalProductGioHang()})\n                        </span>\n                        <Cart \n                            xoaSanPhamGioHang = {this.xoaSanPhamGioHang} \n                            thaydoiSlSanPham = {this.thaydoiSlSanPham}\n                            saveCartProduct = {this.saveCartProduct}\n                            gioHang={this.state.gioHang}/>\n                    </div>\n                </div>\n                <div className=\"row\">\n                    {this.renderProductList()}\n                </div>\n                <div className=\"mt-5\">\n                    {this.renderDetailProduct(detailProduct)}\n                </div>\n            </div>\n        )\n    }\n\n    renderProductList = () => {\n        return mangSanPham.map((item, index) => {\n            return <div className=\"col-4\" key={index}>\n                <BTProduct addCartProduct={this.addCartProduct} xemChiTiet={this.xemChiTiet} product={item}></BTProduct>\n            </div>\n        })\n    }\n\n\n    renderDetailProduct = (detail) => {\n        return <div className=\"row\">\n            <div className=\"col-4\">\n                <h3 className=\"text-center\">{detail.tenSP}</h3>\n                <img src={detail.hinhAnh} height='200'></img>\n            </div>\n\n            <div className=\"col-8\">\n                <h3>Thông số kĩ thuật</h3>\n                <table className=\"table\">\n                    <tr>\n                        <td>Màn hình</td>\n                        <td>{detail.manHinh}</td>\n                    </tr>\n                    <tr>\n                        <td>Hệ điều hành</td>\n                        <td>{detail.heDieuHanh}</td>\n                    </tr>\n                    <tr>\n                        <td>Camera trước</td>\n                        <td>{detail.cameraTruoc}</td>\n                    </tr>\n                    <tr>\n                        <td>Camera sau</td>\n                        <td>{detail.cameraSau}</td>\n                    </tr>\n                    <tr>\n                        <td>RAM</td>\n                        <td>{detail.ram}</td>\n                    </tr>\n                    <tr>\n                        <td>ROM</td>\n                        <td>{detail.rom}</td>\n                    </tr>\n                </table>\n            </div>\n        </div>\n    }\n\n    addCartProduct = (product) => {\n        const spGioHang = {...product, soLuong: 1}\n\n        const productFind = this.state.gioHang.find(item => {\n            return item.maSP === product.maSP;\n        });\n\n        if (productFind) {\n            productFind.soLuong += 1;\n        } else {\n            this.state.gioHang.push(spGioHang);\n        }\n\n        this.setState({gioHang: this.state.gioHang});\n        console.log(this.state.gioHang);\n    }\n\n    getTotalProductGioHang() {\n        // let count = 0;\n        // this.state.gioHang.forEach(item => {\n        //     count += item.soLuong;\n        // });\n\n        // return count;\n        return this.state.gioHang.reduce((tongSL, sp, index) => {\n            return tongSL + sp.soLuong\n        }, 0).toLocaleString()\n    }\n\n    thaydoiSlSanPham = (product, increase) => {\n        const productTimKiem = this.state.gioHang.find((item) => {\n            return item.maSP === product.maSP;\n        });\n\n        if (productTimKiem) {\n            if (increase) {\n                productTimKiem.soLuong += 1;\n            } else {\n                if (productTimKiem.soLuong > 1) {\n                    productTimKiem.soLuong -= 1;\n                }\n            }\n        }\n\n        this.setState({gioHang: this.state.gioHang})\n\n    }\n\n    xoaSanPhamGioHang = (product) => {\n        // const productTimKiem = this.state.gioHang.find((item) => {\n        //     return item.maSP === product.maSP;\n        // });\n\n        // if (productTimKiem) {\n        //     this.state.gioHang.pop(productTimKiem);\n        // }\n\n        // this.setState({gioHang: this.state.gioHang})\n        this.setState({gioHang: this.state.gioHang.filter((item) => {\n            return item.maSP !== product.maSP;\n        })});\n    }\n\n    saveCartProduct = () => {\n        const productTimKiems= this.state.gioHang.filter((item) => {\n            return item.soLuong === 0;\n        });\n\n        console.log(productTimKiems);\n    }\n}\n","/Users/admin/Workspace/CybersStudy/reactjs/sources/reactfe53e-20201211/src/Props/BTDanhSachSP/BTProduct.js",["207"],"import React, { Component } from 'react'\n\nexport default class BTProduct extends Component {\n    product = this.props.product;\n    xemChiTiet = this.props.xemChiTiet;\n    addCartProduct = this.props.addCartProduct;\n\n    render() {\n        return (\n            <div className=\"container\">\n                <div className=\"card bg-light\" style={{ width: 300 }}>\n                    <img className=\"card-img-top\" \n                    src={this.product.hinhAnh} alt=\"Card image\" style={{ maxWidth: '100%', height: 200 }} />\n                    <div className=\"card-body text-center\">\n                        <h4 className=\"card-title text-center\">{this.product.tenSp}</h4>\n                        <p className=\"card-text\">{this.product.gia}</p>\n                        <div className=\"row\">\n                            <button type=\"button\" className=\"btn btn-primary col-6\" onClick={() => {\n                                this.xemChiTiet(this.product)\n                            }}>Chi tiết</button>\n                            <button className=\"btn btn-danger col-6\" onClick={()=> {\n                                this.addCartProduct(this.product)\n                            }}>Thêm giỏ hàng</button>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        )\n    }\n}\n","/Users/admin/Workspace/CybersStudy/reactjs/sources/reactfe53e-20201211/src/Props/BTDanhSachSP/Cart.js",["208"],"import React, { Component } from 'react'\n\nexport default class Cart extends Component {\n\n    thaydoiSlSanPham = this.props.thaydoiSlSanPham;\n    xoaSanPhamGioHang = this.props.xoaSanPhamGioHang;\n    saveCartProduct = this.props.saveCartProduct;\n    render() {\n        return (\n            <div>\n                <div class=\"modal fade\" id=\"modelId\" tabindex=\"-1\" role=\"dialog\" aria-labelledby=\"modelTitleId\" aria-hidden=\"true\">\n                    <div class=\"modal-dialog modal-xl\" role=\"document\">\n                        <div class=\"modal-content\">\n                            <div class=\"modal-header\">\n                                <h5 class=\"modal-title\">Modal title</h5>\n                                <button type=\"button\" class=\"close\" data-dismiss=\"modal\" aria-label=\"Close\">\n                                    <span aria-hidden=\"true\">&times;</span>\n                                </button>\n                            </div>\n                            <div class=\"modal-body\">\n                                <table className=\"table\">\n                                    <thead>\n                                        <tr>\n                                            <th>Mã SP</th>\n                                            <th>Tên SP</th>\n                                            <th>Hình Ảnh</th>\n                                            <th>Số Lượng</th>\n                                            <th>Giá Bán</th>\n                                            <th>Thành Tiền</th>\n                                        </tr>\n                                    </thead>\n                                    <tbody>\n                                        {this.renderGioHang()}\n                                    </tbody>\n                                </table>\n                            </div>\n                            <div class=\"modal-footer\">\n                                <button type=\"button\" class=\"btn btn-secondary\" data-dismiss=\"modal\">Close</button>\n                                <button type=\"button\" class=\"btn btn-primary\" onClick={()=>{\n                                    this.saveCartProduct();\n                                }}>Save</button>\n                            </div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        )\n    }\n\n    renderGioHang = () => {\n        return this.props.gioHang?.map((item, index) => {\n            return <tr key={index}>\n                <td>{item.maSP}</td>\n                <td>{item.tenSP}</td>\n                <td>\n                    <img src={item.hinhAnh} width=\"50\" height=\"50\"></img>\n                </td>\n                <td>\n                    <button className=\"btn btn-primary\" onClick={()=> {\n                        this.thaydoiSlSanPham(item, true)\n                    }}>+</button>\n                    {item.soLuong}\n                    <button className=\"btn btn-primary\" onClick={()=> {\n                        this.thaydoiSlSanPham(item, false)\n                    }}>-</button>\n                </td>\n                <td>{item.giaBan}</td>\n                <td>{(item.giaBan * item.soLuong).toLocaleString()}</td>\n                <td>\n                    <button className=\"btn btn-danger\" onClick= {() => {\n                        this.xoaSanPhamGioHang(item)\n                    }}>Xoá</button>\n                </td>\n            </tr>\n        })\n    }\n}\n","/Users/admin/Workspace/CybersStudy/reactjs/sources/reactfe53e-20201211/src/ReduxExcersice/CartRedux/Shop.js",[],"/Users/admin/Workspace/CybersStudy/reactjs/sources/reactfe53e-20201211/src/ReduxExcersice/CartRedux/ListProduct.js",[],"/Users/admin/Workspace/CybersStudy/reactjs/sources/reactfe53e-20201211/src/ReduxExcersice/CartRedux/CartModel.js",[],"/Users/admin/Workspace/CybersStudy/reactjs/sources/reactfe53e-20201211/src/ReduxExcersice/CartRedux/Product.js",[],"/Users/admin/Workspace/CybersStudy/reactjs/sources/reactfe53e-20201211/src/store/store.js",[],"/Users/admin/Workspace/CybersStudy/reactjs/sources/reactfe53e-20201211/src/store/reducers/cartReducer.js",["209"],"const initialState = {\n    cart: [{\n        id: 1,\n        name: \"Iphone 12 Pro\",\n        image: \"https://cdn.tgdd.vn/Products/Images/42/228744/iphone-12-pro-max-512gb-191020-021004-400x400.png\",\n        price: 44000000,\n        quantity: 3\n    }\n\n    ]\n}\n\n// Reducer: nhan vao action va xu ly\n// export default (state = initialState, action) => {\nexport default (state = initialState, action) => {\n    switch (action.type) {\n        case 'ADD_TO_CART':\n            // const product = { ...payload, quantity: 1 }\n            const cartUpdate = [...state.cart];\n            const productFind = cartUpdate.find(item => {\n                return item.id === action.payload.id;\n            });\n\n            if (productFind) {\n                productFind.quantity += 1;\n            } else {\n                cartUpdate.push(action.payload);\n            }\n            \n            state.cart = cartUpdate;\n            return {...state};\n        case 'DELETE_ITEM':\n            const cartKeep = [...state.cart].filter((item) => {\n                return item.id !== action.payload\n            });\n            state.cart = cartKeep;\n            return { ...state };\n        \n        case 'INCREASE_ENTITY':\n            const product = [...state.cart].find((item) => {\n                return item.id === action.maSP;\n            });\n            if (product) {\n                if (action.soLuong === -1) {\n                    if (product.quantity > 0) {\n                        product.quantity -= 1;\n                    }\n                } else {\n                    product.quantity += 1;\n                }\n            }\n\n            const cartUpdate3 = [...state.cart];\n            state.cart = cartUpdate3;\n            return {...state};\n        default:\n            \n            return { ...state }\n    }\n}","/Users/admin/Workspace/CybersStudy/reactjs/sources/reactfe53e-20201211/src/ReduxExcersice/BTGameXucSac/BTGameXucXac.js",[],"/Users/admin/Workspace/CybersStudy/reactjs/sources/reactfe53e-20201211/src/ReduxExcersice/BTGameXucSac/XucXac.js",["210"],"import React, { Component } from 'react';\nimport {connect} from 'react-redux';\n\nclass XucXac extends Component {\n    render() {\n        return (\n            <div className=\"container\">\n                <div className=\"row\">\n                    <div className=\"col-3\">\n                        <button className=\"btn btn-success d-flex\" onClick={()=> {\n                            this.props.datCuoc(true)\n                        }}>\n                            <span className=\"display-4 p-4\">TÀI</span>\n                        </button>\n                    </div>\n                    <div className=\"col-6 text-center\">\n                        {\n                            this.props.mangXucXac.map((item, index) => {\n                                return <img key={index} src={item.img} width=\"50\" height=\"50\" className=\"ml-2\"/>\n                            })\n                        }\n            \n                    </div>\n                    <div className=\"col-3\">\n                        <button className=\"btn btn-danger d-flex\" onClick={()=> {\n                            this.props.datCuoc(false)\n                        }}>\n                            <span className=\"display-4 p-4\">XỈU</span>\n                        </button>\n                    </div>\n                </div>\n            </div>\n        )\n    }\n}\n\nconst mapStateToProps = (state) => {\n    return {\n        mangXucXac: state.BaiTapGameXucXacReducer.mangXucXac,\n    }\n};\n\nconst mapDispatchToProps = (dispatch) => {\n    return {\n        datCuoc: (isTai) => {\n            const action = {\n                type: 'CHOOSE_STATE_XX',\n                datCuoc: isTai\n            }\n            dispatch(action);\n        }\n    }\n}\n\nexport default connect(mapStateToProps, mapDispatchToProps)(XucXac);","/Users/admin/Workspace/CybersStudy/reactjs/sources/reactfe53e-20201211/src/ReduxExcersice/BTGameXucSac/KetQua.js",[],"/Users/admin/Workspace/CybersStudy/reactjs/sources/reactfe53e-20201211/src/store/reducers/BaiTapGameXucXacReducer.js",["211"],"const initialState = {\n    mangXucXac: [\n        {\n            img: `./imgGame/1.png`,\n            record: 1\n        },\n        {\n            img: `./imgGame/2.png`,\n            record: 2\n        },\n        {\n            img: `./imgGame/3.png`,\n            record: 3\n        }\n    ],\n    \n    soBanThang: 5,\n    tongSoBanChoi: 3,\n    banChon: false,\n}\n\nexport default (state=initialState, action) => {\n    switch(action.type) {\n        case 'PLAY_GAME':\n            const xucXacArray = [];\n            for (let index = 0; index < 3 ; index++) {\n                const min = 1;\n                const max = 7;\n                const rand = Math.floor(min + Math.random() * (max - min));\n                const img = `./imgGame/${rand}.png`;\n                const object = {img, record: rand};\n                if (rand === 6) {\n                    console.log('Random object', object);\n                }\n                \n                xucXacArray.push(object);\n            }\n            state.mangXucXac = xucXacArray;\n\n            // tinh tong diem\n            const tongDiem = state.mangXucXac.reduce((td, xx, index) => {\n                return td += xx.record;\n            }, 0);\n\n            if ((state.banChon && tongDiem > 10) || (!state.banChon && tongDiem <= 10)) {\n                state.soBanThang += 1;\n            }\n\n            state.tongSoBanChoi += 1;\n\n            return {...state};\n        case 'CHOOSE_STATE_XX':\n            state.banChon = action.datCuoc;\n            return {...state}\n        default:\n            return {...state}\n    }\n}","/Users/admin/Workspace/CybersStudy/reactjs/sources/reactfe53e-20201211/src/LifeCycle/LifeCycle.js",[],"/Users/admin/Workspace/CybersStudy/reactjs/sources/reactfe53e-20201211/src/LifeCycle/MoutingAndUpdating.js",[],"/Users/admin/Workspace/CybersStudy/reactjs/sources/reactfe53e-20201211/src/LifeCycle/Unmouting.js",[],"/Users/admin/Workspace/CybersStudy/reactjs/sources/reactfe53e-20201211/src/LifeCycle/Child.js",[],{"ruleId":"212","replacedBy":"213"},{"ruleId":"214","repla